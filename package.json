{
  "name": "react-to-web-api",
  "version": "0.2.1",
  "description": "Consuming a web API with a React client",
  "repository": "https://github.com/dev-academy-challenges/react-to-web-api.git",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "dev": "nodemon --watch server & webpack --watch",
    "dev-client": "webpack-dev-server --hot --inline --content-base public",
    "dev-server": "nodemon --watch server",
    "test": "jest",
    "start": "node server/index"
  },
  "jest": {
    "setupFiles": [
      "./tests/client/setup.js",
      "./tests/client/setup-dom.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "jest": "^24.1.0",
    "jsdom": "^13.2.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "supertest": "^3.4.2",
    "webpack": "^4.29.5",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "superagent": "^4.1.0"
  }
}
