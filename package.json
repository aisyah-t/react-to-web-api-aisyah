{
  "name": "react-to-web-api",
  "version": "0.2.1",
  "description": "A React demo with web APIs",
  "repository": "https://github.com/dev-academy-challenges/react-to-web-api.git",
  "main": "server/index.js",
  "scripts": {
    "dev:client": "webpack --watch",
    "dev:server": "nodemon --watch server",
    "dev": "run-p dev:client dev:server",
    "webpack:server": "webpack-dev-server --hot --inline --content-base public",
    "test": "jest --watchAll",
    "start": "node server"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/client/setup-dom.js"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "supertest": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/register": "^7.10.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.1",
    "sqlite3": "^4.2.0",
    "superagent": "^5.2.2"
  }
